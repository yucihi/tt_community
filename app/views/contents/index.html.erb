<h2>募集一覧</h2>
<table class="table">
<% @contents.each do |content| %>
<tr>
<td>
	<%= link_to content.title, user_content_path(current_user,content) %>
	</td>
	<td><%= link_to "編集", edit_user_content_path(current_user,content)%></td>
	<td><%= link_to "削除", user_content_path(current_user,content),  :method => :delete,data: {confirm: "本当に削除しますか?"} %></td>
	</tr>
	<% end %>
	</table>